 only images\n","CAPTION_XPATHS = [\n","    \"/html/body/div[5]/div[1]/div/div[3]/div/div/div/div/div[2]/div/article/div/div[2]/div/div/div[2]/div[1]/ul/div[1]/li/div/div/div[2]/div[1]/h1\",\n","    \"/html/body/div[4]/div[1]/div/div[3]/div/div/div/div/div[2]/div/article/div/div[2]/div/div/div[2]/div[1]/ul/div[1]/li/div/div/div[2]/div[1]/h1\"\n","]\n","LIKES_XPATHS = [\n","    \"/html/body/div[5]/div[1]/div/div[3]/div/div/div/div/div[2]/div/article/div/div[2]/div/div/div[2]/section[2]/div/div/span/a/span/span\",\n","    \"/html/body/div[4]/div[1]/div/div[3]/div/div/div/div/div[2]/div/article/div/div[2]/div/div/div[2]/section[2]/div/div/span/a/span/span\"\n","]\n","\n","# ---- INIT BROWSER ----\n","driver = webdriver.Chrome()\n","driver.maximize_window()\n","driver.get(\"https://www.instagram.com/accounts/login/\")\n","input(\"Please log in manually, then press Enter here...\")\n","\n","driver.get(TARGET_URL)\n","time.sleep(5 + random.random() * 2)\n","\n","# ---- OPEN CSV ----\n","csv_file = open(\"rhode_posts_selenium.csv\", \"w\", newline=\"\", encoding=\"utf-8\")\n","csv_writer = csv.writer(csv_file)\n","csv_writer.writerow([\"media_type\", \"post_url\", \"likes\", \"caption\"])  # caption at the end\n","\n","# ---- CLICK FIRST POST ----\n","first_post = driver.find_element(By.CLASS_NAME, \"_aagu\")\n","first_post.click()\n","time.sleep(3 + random.random() * 2)\n","\n","posts_scraped = 0\n","\n","while posts_scraped < MAX_POSTS:\n","    try:\n","        post_url = driver.current_url\n","\n","        # ---- Detect media type ----\n","        first_media_type = \"unknown\"\n","        try:\n","            # Check for carousel items\n","            media_items = driver.find_elements(By.XPATH, \"//div[contains(@class,'_aagw')]/ancestor::ul/li\")\n","            if media_items:\n","                first_item = media_items[0]\n","                try:\n","                    first_item.find_element(By.TAG_NAME, \"video\")\n","                    first_media_type = \"video\"\n","                except NoSuchElementException:\n","                    first_media_type = \"image\"  # if not video, carousel item must be image\n","            else:\n","                # Standalone post: search entire post for <video>\n","                try:\n","                    driver.find_element(By.TAG_NAME, \"video\")\n","                    first_media_type = \"video\"\n","                except NoSuchElementException:\n","                    first_media_type = \"image\"\n","        except Exception as e:\n","            first_media_type = \"unknown\"\n","\n","        # ---- Caption ----\n","        caption = \"\"\n","        for xp in CAPTION_XPATHS:\n","            try:\n","                caption_el = driver.find_element(By.XPATH, xp)\n","                caption = caption_el.text.strip().replace(\"\\n\", \" \")\n","                if caption:\n","                    break\n","            except NoSuchElementException:\n","                continue\n","\n","        # ---- Likes ----\n","        likes = \"\"\n","        for xp in LIKES_XPATHS:\n","            try:\n","                likes_el = driver.find_element(By.XPATH, xp)\n","                likes = int(likes_el.text.strip().replace(\",\", \"\"))\n","                if likes:\n","                    break\n","            except (NoSuchElementException, ValueError):\n","                continue\n","\n","        # ---- Write to CSV ----\n","        csv_writer.writerow([first_media_type, post_url, likes, caption])\n","        csv_file.flush()\n","\n","        # ---- Increment counter only for images ----\n","        if first_media_type == \"image\":\n","            posts_scraped += 1\n","\n","        print(f\"[{posts_scraped}] Media: {first_media_type} | {post_url} | Likes: {likes} | Caption: {caption[:40]}...\")\n","\n","        if posts_scraped >= MAX_POSTS:\n","            break\n","\n","        # ---- Next button ----\n","        try:\n","            next_button = WebDriverWait(driver, 5).until(\n","                EC.element_to_be_clickable((By.XPATH, \"//button[contains(@class,'_abl-')]//svg[@aria-label='Next']/..\"))\n","            )\n","        except TimeoutException:\n","            all_buttons = driver.find_elements(By.CLASS_NAME, \"_abl-\")\n","            next_button = None\n","            for btn in all_buttons:\n","                try:\n","                    svg = btn.find_element(By.TAG_NAME, \"svg\")\n","                    if svg.get_attribute(\"aria-label\") == \"Next\":\n","                        next_button = btn\n","                        break\n","                except NoSuchElementException:\n","                    continue\n","            if next_button is None:\n","                print(\"No Next button found, ending scrape.\")\n","                break\n","\n","        next_button.click()\n","        time.sleep(random.uniform(3, 5))\n","\n","    except Exception as e:\n","        print(\"Error navigating posts:\", e)\n","        break\n","\n","print(\"Scraping complete!\")\n","csv_file.close()\n","# driver.quit()"],"metadata":{"id":"4TjpQV7yHph_"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["Trim the dataframe down to the first 500 non-video posts (carousels where the first item is an image are allowed) for the post link, like count, and caption."],"metadata":{"id":"ThwOF0C_I_U6"}},{"cell_type":"code","source":["df = pd.read_csv(\"rhode_posts_selenium.csv\")\n","\n","df_images_only = df[df[\"media_type\"] == \"image\"].reset_index(drop=True)\n","\n","df_images_only\n","\n","df_images_only.to_csv(\"rhode_scraped_images.csv\", index=False)"],"metadata":{"id":"tP5N3JP31Yng"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["Scrape a second time to get the actual direct image links from the original instagram posts."],"metadata":{"id":"cT4mQS6w13Iv"}},{"cell_type":"code","source":["CSV_FILE = \"rhode_image_urls.csv\"\n","\n","options = webdriver.ChromeOptions()\n","driver = webdriver.Chrome(options=options)\n","driver.maximize_window()\n","\n","# Open Instagram for manual login\n","driver.get(\"https://www.instagram.com\")\n","print(\"=\"*60)\n","print(\"Please log into Instagram manually in the browser window.\")\n","print(\"Once you're logged in and see your feed, come back here.\")\n","print(\"=\"*60)\n","print(\"\\nRun the next cell when you're ready to start scraping...\")"],"metadata":{"id":"QB_dnMmV13f_"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["def extract_image_url(post_url):\n","    \"\"\"Extract the scontent image URL from an Instagram post\"\"\"\n","    driver.get(post_url)\n","    time.sleep(3)  # Wait for page to load\n","\n","    try:\n","        # Look for img elements with scontent src\n","        images = driver.find_elements(By.TAG_NAME, \"img\")\n","\n","        for img in images:\n","            src = img.get_attribute(\"src\")\n","            crossorigin = img.get_attribute(\"crossorigin\")\n","\n","            # Check if it matches our criteria\n","            if src and \"scontent\" in src and crossorigin == \"anonymous\":\n","                return src\n","\n","        # Alternative: look specifically in div._aagv\n","        try:\n","            aagv_div = driver.find_element(By.CSS_SELECTOR, \"div._aagv img[crossorigin='anonymous']\")\n","            src = aagv_div.get_attribute(\"src\")\n","            if src and \"scontent\" in src:\n","                return src\n","        except:\n","            pass\n","\n","        return None\n","\n","    except Exception as e:\n","        print(f\"Error extracting image from {post_url}: {e}\")\n","        return None\n","\n","print(\"Functions defined. Ready to scrape!\")\n"],"metadata":{"id":"NqSazUDg2m23"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Read CSV\n","df = pd.read_csv(CSV_FILE)\n","\n","# Add column for image URLs if it doesn't exist\n","if 'image_url' not in df.columns:\n","    df['image_url'] = None\n","\n","# Find first empty row\n","empty_rows = df[df['image_url'].isna() | (df['image_url'] == '')]\n","if len(empty_rows) == 0:\n","    print(\"No empty rows found! All image URLs already extracted.\")\n","else:\n","    start_idx = empty_rows.index[0]\n","    print(f\"Starting from row {start_idx + 1} (first empty image_url)\")\n","    print(f\"Will scrape {len(empty_rows)} posts...\\n\")\n","\n","    # Process each post starting from first empty\n","    for idx, row in df.iloc[start_idx:].iterrows():\n","        # Skip if already has image_url\n","        if pd.notna(row['image_url']) and row['image_url'] != '':\n","            continue\n","\n","        post_url = row['post_url']\n","        print(f\"\\n{'='*60}\")\n","        print(f\"Processing row {idx + 1}/{len(df)}\")\n","        print(f\"URL: {post_url}\")\n","        print('='*60)\n","\n","        # Extract image URL\n","        image_url = extract_image_url(post_url)\n","\n","        if image_url:\n","            print(f\"‚úì Found image URL: {image_url[:80]}...\")\n","            df.at[idx, 'image_url'] = image_url\n","\n","            # Save after each successful extraction (in case of interruption)\n","            df.to_csv(CSV_FILE, index=False)\n","        else:\n","            print(\"‚úó No image URL found\")\n","\n","        # Random human-like delay between 1.5 and 3 seconds\n","        delay = random.uniform(1.5, 3.0)\n","        print(f\"Waiting {delay:.1f} seconds...\")\n","        time.sleep(delay)\n","\n","    print(f\"\\n{'='*60}\")\n","    print(f\"‚úì Saved results to {CSV_FILE}\")\n","    print(f\"‚úì Extracted {len(df[df['image_url'].notna()])} total image URLs\")\n","    print('='*60)\n","\n","driver.quit()\n","print(\"Browser closed!\")\n","\n","# Display first few rows\n","df.head()"],"metadata":{"id":"s5zpkzfa2pC7"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["# **Read csv**"],"metadata":{"id":"2dTPKPn6mS7G"}},{"cell_type":"code","source":["from google.colab import files\n","uploaded = files.upload()"],"metadata":{"id":"UBZ4_mPTmQ-y","colab":{"base_uri":"https://localhost:8080/","height":72},"executionInfo":{"status":"ok","timestamp":1760382584988,"user_tz":300,"elapsed":39327,"user":{"displayName":"Niladri Nath","userId":"15382911335632511896"}},"outputId":"4c460faf-fb77-444d-ed0c-ededa05e2535"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<IPython.core.display.HTML object>"],"text/html":["\n","     <input type=\"file\" id=\"files-60a55f27-3321-4274-bcaf-5c3184b42641\" name=\"files[]\" multiple disabled\n","        style=\"border:none\" />\n","     <output id=\"result-60a55f27-3321-4274-bcaf-5c3184b42641\">\n","      Upload widget is only available when the cell has been executed in the\n","      current browser session. Please rerun this cell to enable.\n","      </output>\n","      <script>// Copyright 2017 Google LLC\n","//\n","// Licensed under the Apache License, Version 2.0 (the \"License\");\n","// you may not use this file except in compliance with the License.\n","// You may obtain a copy of the License at\n","//\n","//      http://www.apache.org/licenses/LICENSE-2.0\n","//\n","// Unless required by applicable law or agreed to in writing, software\n","// distributed under the License is distributed on an \"AS IS\" BASIS,\n","// WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.\n","// See the License for the specific language governing permissions and\n","// limitations under the License.\n","\n","/**\n"," * @fileoverview Helpers for google.colab Python module.\n"," */\n","(function(scope) {\n","function span(text, styleAttributes = {}) {\n","  const element = document.createElement('span');\n","  element.textContent = text;\n","  for (const key of Object.keys(styleAttributes)) {\n","    element.style[key] = styleAttributes[key];\n","  }\n","  return element;\n","}\n","\n","// Max number of bytes which will be uploaded at a time.\n","const MAX_PAYLOAD_SIZE = 100 * 1024;\n","\n","function _uploadFiles(inputId, outputId) {\n","  const steps = uploadFilesStep(inputId, outputId);\n","  const outputElement = document.getElementById(outputId);\n","  // Cache steps on the outputElement to make it available for the next call\n","  // to uploadFilesContinue from Python.\n","  outputElement.steps = steps;\n","\n","  return _uploadFilesContinue(outputId);\n","}\n","\n","// This is roughly an async generator (not supported in the browser yet),\n","// where there are multiple asynchronous steps and the Python side is going\n","// to poll for completion of each step.\n","// This uses a Promise to block the python side on completion of each step,\n","// then passes the result of the previous step as the input to the next step.\n","function _uploadFilesContinue(outputId) {\n","  const outputElement = document.getElementById(outputId);\n","  const steps = outputElement.steps;\n","\n","  const next = steps.next(outputElement.lastPromiseValue);\n","  return Promise.resolve(next.value.promise).then((value) => {\n","    // Cache the last promise value to make it available to the next\n","    // step of the generator.\n","    outputElement.lastPromiseValue = value;\n","    return next.value.response;\n","  });\n","}\n","\n","/**\n"," * Generator function which is called between each async step of the upload\n"," * process.\n"," * @param {string} inputId Element ID of the input file picker element.\n"," * @param {string} outputId Element ID of the output display.\n"," * @return {!Iterable<!Object>} Iterable of next steps.\n"," */\n","function* uploadFilesStep(inputId, outputId) {\n","  const inputElement = document.getElementById(inputId);\n","  inputElement.disabled = false;\n","\n","  const outputElement = document.getElementById(outputId);\n","  outputElement.innerHTML = '';\n","\n","  const pickedPromise = new Promise((resolve) => {\n","    inputElement.addEventListener('change', (e) => {\n","      resolve(e.target.files);\n","    });\n","  });\n","\n","  const cancel = document.createElement('button');\n","  inputElement.parentElement.appendChild(cancel);\n","  cancel.textContent = 'Cancel upload';\n","  const cancelPromise = new Promise((resolve) => {\n","    cancel.onclick = () => {\n","      resolve(null);\n","    };\n","  });\n","\n","  // Wait for the user to pick the files.\n","  const files = yield {\n","    promise: Promise.race([pickedPromise, cancelPromise]),\n","    response: {\n","      action: 'starting',\n","    }\n","  };\n","\n","  cancel.remove();\n","\n","  // Disable the input element since further picks are not allowed.\n","  inputElement.disabled = true;\n","\n","  if (!files) {\n","    return {\n","      response: {\n","        action: 'complete',\n","      }\n","    };\n","  }\n","\n","  for (const file of files) {\n","    const li = document.createElement('li');\n","    li.append(span(file.name, {fontWeight: 'bold'}));\n","    li.append(span(\n","        `(${file.type || 'n/a'}) - ${file.size} bytes, ` +\n","        `last modified: ${\n","            file.lastModifiedDate ? file.lastModifiedDate.toLocaleDateString() :\n","                                    'n/a'} - `));\n","    const percent = span('0% done');\n","    li.appendChild(percent);\n","\n","    outputElement.appendChild(li);\n","\n","    const fileDataPromise = new Promise((resolve) => {\n","      const reader = new FileReader();\n","      reader.onload = (e) => {\n","        resolve(e.target.result);\n","      };\n","      reader.readAsArrayBuffer(file);\n","    });\n","    // Wait for the data to be ready.\n","    let fileData = yield {\n","      promise: fileDataPromise,\n","      response: {\n","        action: 'continue',\n","      }\n","    };\n","\n","    // Use a chunked sending to avoid message size limits. See b/62115660.\n","    let position = 0;\n","    do {\n","      const length = Math.min(fileData.byteLength - position, MAX_PAYLOAD_SIZE);\n","      const chunk = new Uint8Array(fileData, position, length);\n","      position += length;\n","\n","      const base64 = btoa(String.fromCharCode.apply(null, chunk));\n","      yield {\n","        response: {\n","          action: 'append',\n","          file: file.name,\n","          data: base64,\n","        },\n","      };\n","\n","      let percentDone = fileData.byteLength === 0 ?\n","          100 :\n","          Math.round((position / fileData.byteLength) * 100);\n","      percent.textContent = `${percentDone}% done`;\n","\n","    } while (position < fileData.byteLength);\n","  }\n","\n","  // All done.\n","  yield {\n","    response: {\n","      action: 'complete',\n","    }\n","  };\n","}\n","\n","scope.google = scope.google || {};\n","scope.google.colab = scope.google.colab || {};\n","scope.google.colab._files = {\n","  _uploadFiles,\n","  _uploadFilesContinue,\n","};\n","})(self);\n","</script> "]},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Saving rhode-vision-access.json to rhode-vision-access.json\n"]}]},{"cell_type":"code","source":["df = pd.read_csv('instagram_images_final.csv')\n","df.head()"],"metadata":{"id":"dswX3O4DmX7G","colab":{"base_uri":"https://localhost:8080/","height":206},"executionInfo":{"status":"ok","timestamp":1760382592322,"user_tz":300,"elapsed":57,"user":{"displayName":"Niladri Nath","userId":"15382911335632511896"}},"outputId":"378b45b7-ed6f-4a60-8c3e-01f3327268d1"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                                           image_url  likes  \\\n","0  https://scontent-dfw5-2.cdninstagram.com/v/t51...    479   \n","1  https://scontent-dfw5-3.cdninstagram.com/v/t51...  27112   \n","2  https://scontent-dfw5-3.cdninstagram.com/v/t51...  23937   \n","3  https://scontent-dfw5-2.cdninstagram.com/v/t51...  25526   \n","4  https://scontent-dfw5-2.cdninstagram.com/v/t51...  44295   \n","\n","                                             caption  \n","0  prep, set, go ü©∂  peptide eye prep