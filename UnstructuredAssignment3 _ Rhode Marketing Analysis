pinner');\n","          try {\n","            const charts = await google.colab.kernel.invokeFunction(\n","                'suggestCharts', [key], {});\n","          } catch (error) {\n","            console.error('Error during call to suggestCharts:', error);\n","          }\n","          quickchartButtonEl.classList.remove('colab-df-spinner');\n","          quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","        }\n","        (() => {\n","          let quickchartButtonEl =\n","            document.querySelector('#df-5eefac14-c933-4ee1-abb5-79668d339e24 button');\n","          quickchartButtonEl.style.display =\n","            google.colab.kernel.accessAllowed ? 'block' : 'none';\n","        })();\n","      </script>\n","    </div>\n","\n","    </div>\n","  </div>\n"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"dataframe","summary":"{\n  \"name\": \"df[['likes', 'binary']]\",\n  \"rows\": 10,\n  \"fields\": [\n    {\n      \"column\": \"likes\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 19895,\n        \"min\": 479,\n        \"max\": 65367,\n        \"num_unique_values\": 10,\n        \"samples\": [\n          30471,\n          27112,\n          65367\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    },\n    {\n      \"column\": \"binary\",\n      \"properties\": {\n        \"dtype\": \"number\",\n        \"std\": 0,\n        \"min\": 0,\n        \"max\": 1,\n        \"num_unique_values\": 2,\n        \"samples\": [\n          1,\n          0\n        ],\n        \"semantic_type\": \"\",\n        \"description\": \"\"\n      }\n    }\n  ]\n}"}},"metadata":{},"execution_count":12}]},{"cell_type":"markdown","source":["# **Task D:  Logistic Regression**"],"metadata":{"id":"jLFUpUjkIl1l"}},{"cell_type":"code","source":["# (D0.1) Make a text column from Vision labels: if your Task B stored lists -> join them\n","def join_labels_cell(x):\n","    # handles: NaN, lists, strings\n","    if isinstance(x, list):\n","        return \" \".join(map(str, x))\n","    if pd.isna(x):\n","        return \"\"\n","    return str(x)\n","\n","df['labels_text'] = df['labels'].apply(join_labels_cell)       # from Task B output\n","df['caption_text'] = df['caption'].fillna(\"\").astype(str) # ensure string\n","\n","# (D0.2) Define X/y and drop rows with missing target\n","model_df = df.dropna(subset=['binary']).copy()\n","X_labels_only   = model_df[['labels_text']]\n","X_captions_only = model_df[['caption_text']]\n","y = model_df['binary'].astype(int)\n","\n","# ---------- (D1) UTIL: print metrics ----------\n","def print_metrics(y_true, y_pred, title=\"\"):\n","    cm = confusion_matrix(y_true, y_pred, labels=[0,1])\n","    acc = accuracy_score(y_true, y_pred)\n","    print(f\"\\n=== {title} ===\")\n","    print(\"Confusion Matrix [rows=true, cols=pred]:\\n\", cm)\n","    print(f\"Accuracy: {acc:.4f}\")\n","    # Optional: detailed per-class metrics (precision/recall/F1)\n","    # print(\"\\n\" + classification_report(y_true, y_pred, digits=3))\n","\n","\n","# ================================\n","# (D2) IMAGE LABELS ONLY MODEL\n","# ================================\n","# (D2.1) Train/test split (stratify to keep 0/1 ratio stable)\n","Xtr, Xte, ytr, yte = train_test_split(\n","    X_labels_only, y, test_size=0.2, random_state=42, stratify=y\n",")\n","\n","# (D2.2) Pipeline: TF-IDF over 'labels_text' -> Logistic Regression\n","labels_clf = Pipeline(steps=[\n","    # 1) vectorize labels text\n","    (\"tfidf\", TfidfVectorizer(lowercase=True, stop_words=\"english\", min_df=2)),\n","    # 2) logistic regression classifier\n","    (\"logit\", LogisticRegression(max_iter=2000, solver=\"liblinear\"))\n","])\n","\n","# (D2.3) Fit & evaluate\n","labels_clf.fit(Xtr['labels_text'], ytr)\n","y_pred_labels = labels_clf.predict(Xte['labels_text'])\n","print_metrics(yte, y_pred_labels, title=\"Image Labels Only\")\n","\n","\n","# ================================\n","# (D3) CAPTION ONLY MODEL\n","# ================================\n","Xtr_c, Xte_c, ytr_c, yte_c = train_test_split(\n","    X_captions_only, y, test_size=0.2, random_state=42, stratify=y\n",")\n","\n","captions_clf = Pipeline(steps=[\n","    (\"tfidf\", TfidfVectorizer(lowercase=True, stop_words=\"english\",\n","                              min_df=2, max_df=0.95)),\n","    (\"logit\", LogisticRegression(max_iter=2000, solver=\"liblinear\"))\n","])\n","\n","captions_clf.fit(Xtr_c['caption_text'], ytr_c)\n","y_pred_captions = captions_clf.predict(Xte_c['caption_text'])\n","print_metrics(yte_c, y_pred_captions, title=\"Captions Only\")\n","\n","\n","# ================================\n","# (D4) COMBINED MODEL (LABELS + CAPTION)\n","# ================================\n","# (D4.1) Use a ColumnTransformer to TF-IDF both columns and concatenate features\n","X_both = model_df[['labels_text', 'caption_text']]\n","Xtr_b, Xte_b, ytr_b, yte_b = train_test_split(\n","    X_both, y, test_size=0.2, random_state=42, stratify=y\n",")\n","\n","text_union = ColumnTransformer(\n","    transformers=[\n","        # 1) TF-IDF on labels_text\n","        (\"labels_tfidf\", TfidfVectorizer(lowercase=True, stop_words=\"english\", min_df=2),\n","         \"labels_text\"),\n","        # 2) TF-IDF on caption_text\n","        (\"caption_tfidf\", TfidfVectorizer(lowercase=True, stop_words=\"english\",\n","                                          min_df=2, max_df=0.95),\n","         \"caption_text\"),\n","    ],\n","    remainder='drop'\n",")\n","\n","combined_clf = Pipeline(steps=[\n","    (\"feats\", text_union),\n","    (\"logit\", LogisticRegression(max_iter=3000, solver=\"liblinear\"))\n","])\n","\n","combined_clf.fit(Xtr_b, ytr_b)\n","y_pred_both = combined_clf.predict(Xte_b)\n","print_metrics(yte_b, y_pred_both, title=\"Combined (Labels + Captions)\")"],"metadata":{"id":"jH1BAg5PHpr6","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1760383300495,"user_tz":300,"elapsed":141,"user":{"displayName":"Niladri Nath","userId":"15382911335632511896"}},"outputId":"a670894d-1069-4f8c-81a1-6a03ef7c9ed6"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","=== Image Labels Only ===\n","Confusion Matrix [rows=true, cols=pred]:\n"," [[29 20]\n"," [19 30]]\n","Accuracy: 0.6020\n","\n","=== Captions Only ===\n","Confusion Matrix [rows=true, cols=pred]:\n"," [[36 13]\n"," [11 38]]\n","Accuracy: 0.7551\n","\n","=== Combined (Labels + Captions) ===\n","Confusion Matrix [rows=true, cols=pred]:\n"," [[36 13]\n"," [14 35]]\n","Accuracy: 0.7245\n"]}]},{"cell_type":"markdown","source":["In Task C, we calculated the median number of likes across all posts and created a binary variable for what \"High Engagement\" and \"Low Engagement\" are. Any post above the median = 1, or \"High Engagement\". Any post below the median = 0, or \"Low Engagement\". In Task D, we used text-based features from images and captions to predict whether a post will get high or low engagement.\n","\n","We trained three separate logistic-regression models using TF-IDF features. The accuracy tells us the percentage of posts the model classified correctly. The confusion matrix tells us where our model is right or wrong and whether our model is correctly predicting both classes equally or leaning towards one side.\n","\n","**For Image Labels only:**\n","\n","The model correctly identified 29 low-engagement and 30 high-engagement posts.\n","It misclassified 20 low posts as high (false positives) and 19 high posts as low (false negatives).\n","Performance is roughly balanced between classes but limited overall, showing that visual labels alone don‚Äôt capture the emotional or contextual factors that drive likes.\n","\n","**For Captions only:**\n","\n","The model correctly predicted most examples in both classes, with only 24 errors total.\n","Stronger performance reflects that caption text carries richer engagement cues ‚Äî brand tone, emojis, and promotional language directly influence user behavior.\n","\n","**For Combined:**\n","\n","Similar balance between true positives and true negatives, but slightly more false negatives than the caption-only model.\n","Combining features adds some noise from repetitive or generic image labels, leading to a small dip relative to captions alone.\n","The first model was trained using Image Labels only and had an accuracy of 0.60. The second model was trained using Caption only and had an accuracy of 0.76. The third model was trained using both Image Labels and Captions combined and had an accuracy of 0.72.\n","\n","We see that the Caption-based model outperforms the Image only model which shows that language can convey emotional and promotional cues better than using Images only. Written context includes mentions, hashtags, calls-to-action, and promo wording compared to there being less diversity in just labels. Combined underperforms Captions only likely due to feature noise from Image labels as well as imbalanced feature scaling. The image labels could have introduced repetitive or generic words that added noise. The combined model's performance may improve with more data or better visual feature engineering.\n"],"metadata":{"id":"CLBVovCebFOl"}},{"cell_type":"markdown","source":["# **Task E: Topic Modeling**"],"metadata":{"id":"C6TmG5wmIqsT"}},{"cell_type":"code","source":["#LDA on the image labels\n","#prepare corpus for LDA (counts)\n","df['labels_text'] = df['labels_text'].fillna(\"\").astype(str)\n","\n","cv = CountVectorizer(lowercase=True, stop_words='english', min_df=2)\n","X_counts = cv.fit_transform(df['labels_text'])\n","print(\"Count matrix shape:\", X_counts.shape)\n","\n","#fit LDA ‚Äî start with 5 topics\n","n_topics = 5\n","lda = LatentDirichletAllocation(\n","    n_components=n_topics,\n","    random_state=42,\n","    learning_method='batch',\n","    max_iter=20 )\n","lda.fit(X_counts)\n","print(f\"LDA fitted with {n_topics} topics\")\n","\n","#inspecting top words per topic and name topics\n","def top_words_per_topic(model, feature_names, n_top_words=10):\n","    topic_tops = []\n","    for k, comp in enumerate(model.components_):\n","        inds = comp.argsort()[:-n_top_words-1:-1]\n","        words = [feature_names[i] for i in inds]\n","        topic_tops.append(words)\n","        print(f\"Topic {k+1}: {', '.join(words)}\")\n","    return topic_tops\n","\n","feature_names = cv.get_feature_names_out()\n","topic_top_words = top_words_per_topic(lda, feature_names, n_top_words=10)\n","\n","topic_names = [\n","    \"Body & Apparel Focus\",           # Topic 1\n","    \"Beauty & Product Care\",          # Topic 2\n","    \"Accessories & Fashion Items\",    # Topic 3\n","    \"Human Models\",# Topic 4\n","    \"Makeup & Facial Features\"        # Topic 5\n","]\n","\n","#get topic weights per post and add to df\n","topic_weights = lda.transform(X_counts)  # rows=sum‚âà1 across topics\n","topic_cols = [f\"topic_{i+1}\" for i in range(n_topics)]\n","for i, col in enumerate(topic_cols):\n","    df[col] = topic_weights[:, i]\n","\n","print(\"Topic columns added:\", topic_cols)"],"metadata":{"id":"zYtQYINKHpxm","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1760384771969,"user_tz":300,"elapsed":1174,"user":{"displayName":"Niladri Nath","userId":"15382911335632511896"}},"outputId":"e98b6e83-6314-41a7-9a2e-5c5df26a9fc7"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Count matrix shape: (489, 341)\n","LDA fitted with 5 topics\n","Topic 1: nail, undergarment, swimsuit, lingerie, bra, advertising, cocktail, abdomen, active, red\n","Topic 2: care, personal, cosmetics, plastic, bottle, lip, skin, label, lipstick, pink\n","Topic 3: bag, leather, luggage, dress, shoulder, bags, plumbing, handbag, fixture, photography\n","Topic 4: model, fashion, hair, device, fur, waist, gadget, beauty, shoot, photo\n","Topic 5: lips, eyelash, hair, eyebrow, cosmetics, brown, lipstick, eye, beauty, skin\n","Topic columns added: ['topic_1', 'topic_2', 'topic_3', 'topic_4', 'topic_5']\n"]}]},{"cell_type":"code","source":["# assumes: lda, X_counts, n_topics, topic_names, topic_cols already defined in Part 1\n","# topic_weights = lda.transform(X_counts) and topic_cols = [f\"topic_{i+1}\" for i in range(n_topics)]\n","\n","weights_df = pd.DataFrame(topic_weights, columns=topic_cols, index=df.index)\n","\n","#dominant topic for each post (argmax) and the top2 separation\n","top2 = np.sort(weights_df.values, axis=1)[:, -2:]            # last two columns = top-2 weights\n","separation = top2[:, 1] - top2[:, 0]                         # Top1 - Top2\n","dominant_idx = weights_df.values.argmax(axis=1)              # argmax topic idx (0..n_topics-1)\n","\n","df['dominant_topic_id'] = dominant_idx + 1                   # 1..n_topics\n","df['dominant_topic_name'] = [topic_names[i] for i in dominant_idx]\n","df['top1_weight'] = top2[:, 1]\n","df['top2_weight'] = top2[:, 0]\n","df['topic_separation'] = separation                          # larger = cleaner assignment\n","\n","#merge the full weight matrix back for easy viewing/CSV\n","for c in topic_cols:\n","    df[c] = weights_df[c]\n","\n","#preview the first few rows with key fields\n","cols_preview = ['image_url','likes','dominant_topic_name','top1_weight','top2_weight','topic_separation'] + topic_cols\n","display(df[cols_preview].sort_values(by = 'top1_weight', ascending = False).head(10))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":363},"id":"v9TOtw7fKvqq","executionInfo":{"status":"ok","timestamp":1760386193668,"user_tz":300,"elapsed":65,"user":{"displayName":"Niladri Nath","userId":"15382911335632511896"}},"outputId":"1b7492ba-ac37-4744-f110-cf2b1d3271bc"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["                                             image_url   likes  \\\n","75   https://scon