. These labels were stored as lists in a new `labels` column. We also dropped rows with missing, null, or invalid label values."],"metadata":{"id":"PsFhEtfNQMhz"}},{"cell_type":"code","source":["# set up credentials\n","os.environ[\"GOOGLE_APPLICATION_CREDENTIALS\"] = \"rhode-vision-access.json\"\n","\n","# initialize Vision client\n","client = vision.ImageAnnotatorClient()\n","\n","def get_labels(image_url):\n","    try:\n","        # download the image bytes using requests\n","        response = requests.get(image_url, timeout=10)\n","        response.raise_for_status()  # raise error for bad status\n","        image_content = response.content\n","\n","        # load image content into Vision API\n","        image = vision.Image(content=image_content)\n","        result = client.label_detection(image=image)\n","\n","        labels = [label.description for label in result.label_annotations]\n","        return labels if labels else None\n","\n","    except Exception as e:\n","        print(f\"Error for {image_url[:50]}...: {e}\")\n","        return None\n","\n","df['labels'] = df['image_url'].apply(get_labels)\n","df = df[~df['labels'].isin([None, np.nan])]  # drop all rows where labels are None & NaN"],"metadata":{"id":"pyfAgKgNI_uE"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["df['labels']"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":458},"id":"sGOjJTyNN3-p","executionInfo":{"status":"ok","timestamp":1760383285409,"user_tz":300,"elapsed":32,"user":{"displayName":"Niladri Nath","userId":"15382911335632511896"}},"outputId":"0959dfe1-782b-490a-ab94-efc9484cf73c"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["0      [Sunglasses, Blazer, Denim, Goggles, Album cov...\n","1      [Lips, Jaw, Eyelash, Finger, Tooth, Blond, Nai...\n","2      [Hair, Lips, Beauty, Black hair, Eye, Fashion,...\n","3      [Eyelash, Brown, Eye shadow, Cosmetics, Glitte...\n","4                             