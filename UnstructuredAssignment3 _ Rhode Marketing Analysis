kes'], errors='coerce')\n","df_sorted = df.sort_values('likes', ascending=False).reset_index(drop=True)\n","\n","# compute quartiles on raw likes\n","q1, q3 = df_sorted['likes'].quantile([0.25, 0.75])\n","\n","# label groups purely by likes (no binary)\n","df_sorted['engagement_group'] = pd.cut(\n","    df_sorted['likes'],\n","    bins=[-np.inf, q1, q3, np.inf],\n","    labels=['Low', 'Mid', 'High'] )\n","\n","# slice top & bottom quartiles\n","top_q = df_sorted[df_sorted['engagement_group'] == 'High']\n","low_q = df_sorted[df_sorted['engagement_group'] == 'Low']\n","\n","# average topic weights across quartiles\n","avg_top = top_q[topic_cols].mean()\n","avg_low = low_q[topic_cols].mean()\n","diff = (avg_top - avg_low)\n","\n","# build results table\n","topic_compare = pd.DataFrame({\n","    \"Topic\": [f\"Topic {i+1}\" for i in range(len(topic_cols))],\n","    \"Name\": topic_names,  # use descriptive names from Part 1\n","    \"High-Likes Avg Weight\": avg_top.values,\n","    \"Low-Likes Avg Weight\": avg_low.values,\n","    \"Difference (High ‚Äì Low)\": diff.values,\n","    \"Top Words (for naming)\": [\", \".join(words) for words in topic_top_words]\n","}).sort_values(\"Difference (High ‚Äì Low)\", ascending=False).reset_index(drop=True)\n","\n","print(\"\\n=== Topic Comparison: High vs Low (by raw likes quartiles) ===\")\n","display(topic_compare)\n","\n","# bar chart visualization\n","plot_cols = [\"High-Likes Avg Weight\", \"Low-Likes Avg Weight\"]\n","topic_compare.set_index(\"Name\")[plot_cols].plot.bar(figsize=(9,5), width=0.8, alpha=0.9)\n","plt.title(\"Average Topic Weights: High vs Low (by raw likes)\")\n","plt.ylabel(\"Average Topic Probability\")\n","plt.xlabel(\"Topic\")\n","plt.xticks(rotation=0)\n","plt.tight_layout()\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":731},"id":"QVF_4kf0CuYg","executionInfo":{"status":"ok","timestamp":1760384775188,"user_tz":300,"elapsed":523,"user":{"displayName":"Niladri Nath","u